    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/status-im/status-desktop/releases/download/$match1/StatusIm-Desktop-v$version.7z",
                "hash": {
                    "url": "$baseurl/StatusIm-Desktop-v$version.sha256",
                    "regex": "$sha256\\s+$fname"
                }
            }
        }
    }
