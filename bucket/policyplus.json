    "autoupdate": {
        "url": "https://s3-us-west-2.amazonaws.com/policy-plus/Policy%20Plus.exe#/PolicyPlus.exe",
        "hash": {
            "url": "$baseurl",
            "regex": "$basename.*?\\</LastModified\\>\\<ETag\\>&quot;$md5"
        }
    }
