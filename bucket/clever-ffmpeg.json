            "(Invoke-WebRequest 'https://www.videohelp.com/software/clever-FFmpeg-GUI/version-history') -match '\\s+version\\s+(?<version>[\\d.]+)\\.' | Out-Null",
            "$siteData += ', ' + $Matches['version']",
            "Write-Output \"$siteData\""
        ],
        "regex": "(?<Code>[\\w]),\\s([\\d.]+)"
    },
